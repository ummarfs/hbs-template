<!DOCTYPE html>
<html lang="en" class="scroll-smooth">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dr. Evelyn Reed - Integrative Health</title>

  <script src="https://cdn.tailwindcss.com"></script>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

{{#if fontPicker.value.fontPicker.heading}}
<link href="{{fontPicker.value.fontPicker.heading.url}}" rel="stylesheet">
{{else}}
<link href="https://fonts.googleapis.com/css2?family=Forum:wght@400;700&display=swap" rel="stylesheet">
{{/if}}
{{#if fontPicker.value.fontPicker.body}}
<link href="{{fontPicker.value.fontPicker.body.url}}" rel="stylesheet">
{{else}}
<link href="https://fonts.googleapis.com/css2?family=DM+Sans:wght@400;700&display=swap" rel="stylesheet">
{{/if}}
{{#if fontPicker.value.fontPicker.nav}}
<link href="{{fontPicker.value.fontPicker.nav.url}}" rel="stylesheet">
{{else}}
<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;700&display=swap" rel="stylesheet">
{{/if}}
{{#if fontPicker.value.fontPicker.accent}}
<link href="{{fontPicker.value.fontPicker.accent.url}}" rel="stylesheet">
{{else}}
<link href="https://fonts.googleapis.com/css2?family=Raleway:wght@400;700&display=swap" rel="stylesheet">
{{/if}}

  <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">

  <style>
    :root {
      --background: {{default colorPalette.value.colorPalette.background '#083344'}};
      --surface: {{default colorPalette.value.colorPalette.surface 'rgba(17, 94, 119, 0.1)'}};
      --accent: {{default colorPalette.value.colorPalette.accent '#fdba74'}};
      --accent-hover: {{default colorPalette.value.colorPalette.accent-hover '#fb923c'}};
      --text-primary: {{default colorPalette.value.colorPalette.text-primary '#ffffff'}};
      --text-secondary: {{default colorPalette.value.colorPalette.text-secondary 'rgba(255, 255, 255, 0.8)'}};
      --border: {{default colorPalette.value.colorPalette.border 'rgba(255, 255, 255, 0.1)'}};
      --font-heading: {{default fontPicker.value.fontPicker.heading.family 'Forum'}};
      --font-body: {{default fontPicker.value.fontPicker.body.family 'DM Sans'}};
      --font-nav: {{default fontPicker.value.fontPicker.nav.family 'Poppins'}};
      --font-accent: {{default fontPicker.value.fontPicker.accent.family 'Raleway'}};
    }

    body {
      font-family: var(--font-body);
      color: var(--text-primary);
      background-color: var(--background);
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }

    .font-heading {
      font-family: var(--font-heading);
    }

    .font-body {
      font-family: var(--font-body);
    }

    .font-nav {
      font-family: var(--font-nav);
    }

    .font-accent {
      font-family: var(--font-accent);
    }
  </style>
</head>

<body>

  <header class="sticky top-0 z-50 bg-[var(--background)] bg-opacity-90 backdrop-blur-md border-b border-[var(--border)]">
    <nav class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="flex justify-between items-center h-20">

        <a href="/" class="text-2xl font-heading tracking-[2px]">
          {{personal_info.value.firstName}} {{personal_info.value.lastName}}
        </a>

        <div class="hidden md:flex items-center space-x-8">
          {{#if about.value.about.content}}
          {{#unless about.meta.hide}}
          <a href="#about"
            class="text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default about.value.about.navbarName 'About'}}</a>
          {{/unless}}
          {{/if}}
          {{#if services.value.services.items}}
          {{#unless services.meta.hide}}
          <a href="#services"
            class="text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default services.value.services.navbarName 'Services'}}</a>
          {{/unless}}
          {{/if}}
            {{#if gallery.value.gallery.items}}
            {{#unless gallery.meta.hide}}
          <a href="#gallery"
            class="text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default gallery.value.gallery.navbarName 'Gallery'}}</a>
            {{/unless}}
            {{/if}}
            {{#if testimonials.value.testimonials.items}}
            {{#unless testimonials.meta.hide}}
          <a href="#testimonials"
            class="text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default testimonials.value.testimonials.navbarName 'Testimonials'}}</a>
            {{/unless}}
            {{/if}}
            {{#if faq.value.faq.items}}
            {{#unless faq.meta.hide}}
          <a href="#faq"
            class="text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default faq.value.faq.navbarName 'FAQ'}}</a>
            {{/unless}}
            {{/if}}
            {{#if contacts.value.contacts}}
            {{#unless contacts.meta.hide}}
          <a href="#contact"
            class="font-accent text-sm font-semibold text-[var(--accent)] border border-[var(--accent)] rounded-lg px-6 py-3 hover:bg-[var(--accent)] hover:text-[var(--background)] transition-colors">
            {{default contacts.value.contacts.buttonText 'Book Appointment'}}
          </a>
          {{/unless}}
          {{/if}}
        </div>

        <button id="menu-button" class="md:hidden p-2 rounded-md hover:bg-[var(--border)] transition-colors"
          aria-label="Toggle menu" aria-expanded="false">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
            stroke="currentColor" class="w-6 h-6">
            <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
          </svg>
        </button>

      </div>
    </nav>

    <div id="mobile-menu" class="hidden md:hidden bg-[var(--background)] border-t border-[var(--border)]">
      <div class="flex flex-col space-y-4 p-6">
        {{#if about.value.about.content}}
        {{#unless about.meta.hide}}
        <a href="#about"
          class="block text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default about.value.about.navbarName 'About'}}</a>
        {{/unless}}
        {{/if}}
        {{#if services.value.services.items}}
        {{#unless services.meta.hide}}
        <a href="#services"
          class="block text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default services.value.services.navbarName 'Services'}}</a>
        {{/unless}}
        {{/if}}
        {{#if gallery.value.gallery.items}}
        {{#unless gallery.meta.hide}}
        <a href="#gallery"
          class="block text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default gallery.value.gallery.navbarName 'Gallery'}}</a>
        {{/unless}}
        {{/if}}
        {{#if testimonials.value.testimonials.items}}
        {{#unless testimonials.meta.hide}}
        <a href="#testimonials"
          class="block text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default testimonials.value.testimonials.navbarName 'Testimonials'}}</a>
        {{/unless}}
        {{/if}}
        {{#if faq.value.faq.items}}
        {{#unless faq.meta.hide}}
        <a href="#faq"
          class="block text-sm font-nav tracking-wider uppercase hover:text-[var(--accent)] transition-colors">{{default faq.value.faq.navbarName 'FAQ'}}</a>
        {{/unless}}
        {{/if}}
        {{#if contacts.value.contacts}}
        {{#unless contacts.meta.hide}}
        <a href="#contact"
          class="block text-center mt-4 font-accent text-sm font-semibold text-[var(--accent)] border border-[var(--accent)] rounded-lg px-6 py-3 hover:bg-[var(--accent)] hover:text-[var(--background)] transition-colors">
        {{default contacts.value.contacts.buttonText 'Book Appointment'}}
        </a>
        {{/unless}}
        {{/if}}
      </div>
    </div>
  </header>

  <main>

    {{#if hero.value.hero}}
    {{#unless hero.meta.hide}}
    <section class="min-h-screen flex items-center justify-center text-center -mt-20 py-20">
      <div class="max-w-4xl mx-auto px-4">
        <h2 class="text-3xl md:text-4xl font-heading tracking-wide text-[var(--text-secondary)]" data-aos="fade-up">
          {{#if hero.value.hero.subheading}}
          {{hero.value.hero.subheading}}
          {{else}}
          Welcome to the Practice of {{personal_info.value.firstName}} {{personal_info.value.lastName}}
          {{/if}}
        </h2>
        <h1 class="text-5xl sm:text-6xl md:text-7xl font-heading tracking-wide text-[var(--accent)] mt-4" data-aos="fade-up"
          data-aos-delay="200">
          {{default hero.value.hero.heading 'YOUR PARTNER IN HEALTH & WELLNESS'}}
        </h1>
      </div>
    </section>
    {{/unless}}
    {{/if}}

   {{#if banner.value.banner}}
   {{#unless banner.meta.hide}}
    <section class="h-96 bg-cover bg-fixed bg-center"
      style="background-image: url('{{default banner.value.banner.image 'https://images.unsplash.com/photo-1579684385127-1ef15d508118?ixlib=rb-4.0.3&auto=format&fit=crop&w=2370&q=80'}}');">
    </section>
    {{/unless}}
    {{/if}}

{{#if about.value.about.content}}
{{#unless about.meta.hide}}
    <section id="about" class="py-24 sm:py-32">
      <div class="max-w-3xl mx-auto px-4 text-center" data-aos="fade-up">
        <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)]">
        {{default about.value.about.section_title 'Our Philosophy of Care'}}
        </h2>
        <div class="text-base md:text-lg text-[var(--text-secondary)] leading-relaxed space-y-6 mt-8">
          <p>
            {{!-- Our practice is built on the philosophy that true wellness comes from a balance of mind, body, and spirit.
            We provide a welcoming, modern environment where you are heard, respected, and empowered to take an active
            role in your health journey. --}}
            {{about.value.about.content}}
          </p>
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if practitioner.value.practitioner.content}}
    {{#unless practitioner.meta.hide}}
    <section class="py-24 sm:py-32 bg-[var(--surface)]">
      <div class="max-w-6xl mx-auto px-4">
        <div class="grid grid-cols-1 md:grid-cols-2 gap-12 items-center" data-aos="fade-up">
          <div data-aos="zoom-in">
            {{#if practitioner.value.practitioner.image}}
            <img
              src="{{practitioner.value.practitioner.image}}"
              alt="Dr. Evelyn Reed" class="rounded-lg shadow-2xl object-cover w-full h-full">
              {{else}}
              {{#if personal_info.value.profilePicureUrl}}
              <img src="{{personal_info.value.profilePicureUrl}}"
                alt="Dr. Evelyn Reed" class="rounded-lg shadow-2xl object-cover w-full h-full">
                {{else}}
                <img src="https://images.unsplash.com/photo-1537368910025-700350fe46c7?ixlib=rb-4.0.3&auto=format&fit=crop&w=2370&q=80"
                  alt="Dr. Evelyn Reed" class="rounded-lg shadow-2xl object-cover w-full h-full">
                  {{/if}}
                  {{/if}}
          </div>
          <div data-aos="fade-left" data-aos-delay="200">
            <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)]">
              {{#if practitioner.value.practitioner.section_title}}
             {{practitioner.value.practitioner.section_title}}
              {{else}}
              Meet {{personal_info.value.firstName}} {{personal_info.value.lastName}}
              {{/if}}
            </h2>
            <p class="text-base md:text-lg text-[var(--text-secondary)] leading-relaxed mt-6">
              {{practitioner.value.practitioner.content}}
              {{!-- Dr. Evelyn Reed is a board-certified specialist in integrative health, dedicated to providing
              patient-centered care that addresses the whole person. With over 15 years of experience, she combines
              conventional medicine with evidence-based holistic therapies to help you achieve optimal health. --}}
            </p>
            {{#if practitioner.value.practitioner.achievements}}
            <ul class="space-y-4 mt-8 text-[var(--text-secondary)]">
              {{#each practitioner.value.practitioner.achievements}}
              <li class="flex items-center">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                  stroke="currentColor" class="w-5 h-5 mr-3 text-[var(--accent)] flex-shrink-0">
                  <path stroke-linecap="round" stroke-linejoin="round" d="m4.5 12.75 6 6 9-13.5" />
                </svg>
                {{this}}
              </li>
              {{/each}}
            </ul>
            {{/if}}
          </div>
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

   {{#unless callout_banner.meta.hide}}
    <section class="py-48 bg-cover bg-fixed bg-center text-center relative"
      style="background-image: url('{{default callout_banner.value.callout_banner.image 'https://images.unsplash.com/photo-1544377193-33dcf4d68fb5?ixlib=rb-4.0.3&auto=format&fit=crop&w=2370&q=80'}}');">
      <div class="absolute inset-0 bg-black opacity-50"></div>
      <div class="max-w-4xl mx-auto px-4" data-aos="fade-in" data-aos-duration="1000">
        <h1 class="text-5xl sm:text-6xl md:text-7xl font-heading tracking-wide">
         {{default callout_banner.value.callout_banner.title 'ADVANCED SOLUTIONS, PERSONALIZED CARE'}}
        </h1>
        <h4 class="text-xl md:text-2xl font-body tracking-wide mt-4">
          {{default callout_banner.value.callout_banner.subtitle 'Integrative Health Specialist, New York'}}
        </h4>
      </div>
    </section>
    {{/unless}}

    {{#if approach.value.approach.items}}
    {{#unless approach.meta.hide}}
    <section id="approach" class="py-24 sm:py-32">
      <div class="max-w-6xl mx-auto px-4" data-aos="fade-up">
        <div class="grid grid-cols-1 md:grid-cols-3 gap-12 items-center">
          <div class="md:col-span-1 mb-12 md:mb-0">
            <h1 class="text-5xl sm:text-6xl font-heading text-[var(--accent)] leading-tight text-center md:text-left">
              {{default approach.value.approach.section_title 'OUR COMMITMENT TO YOUR HEALTH'}}
            </h1>
          </div>
          <div class="md:col-span-2">
            <div class="grid grid-cols-1 sm:grid-cols-2 gap-x-8 gap-y-12">
              {{#each approach.value.approach.items}}
              <div class="text-center" data-aos="zoom-in" data-aos-delay="100">
                <h3 class="text-5xl font-heading text-[var(--accent)]">{{this.text}}</h3>
                <p class="text-sm font-body text-[var(--text-secondary)] mt-2">{{this.subtext}}</p>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if services.value.services.items}}
    {{#unless services.meta.hide}}
    <section id="services" class="py-24 sm:py-32 bg-[var(--surface)]">
      <div class="max-w-6xl mx-auto px-4">
        <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)] text-center mb-16" data-aos="fade-up">
         {{default services.value.services.section_title 'Our Services'}}
        </h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-10">
          {{#each services.value.services.items}}
          <div class="border border-[var(--border)] rounded-lg p-8" data-aos="fade-up" data-aos-delay="100">
            <h3 class="text-2xl font-heading text-[var(--accent)] mb-4">{{this.title}}</h3>
            <p class="text-[var(--text-secondary)] leading-relaxed">
              {{this.description}}
            </p>
          </div>
          {{/each}}
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if gallery.value.gallery.items}}
    {{#unless gallery.meta.hide}}
    <section id="gallery" class="py-24 sm:py-32">
      <div class="max-w-6xl mx-auto px-4">
        <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)] text-center mb-16" data-aos="fade-up">
          {{default gallery.value.gallery.section_title 'Clinic Gallery'}}
        </h2>
        <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-6">
         {{#each gallery.value.gallery.items}}
          <div class="overflow-hidden rounded-lg shadow-lg" data-aos="zoom-in" data-aos-delay="100">
            <img
              src="{{this}}"
              alt="image {{@index}}"
              class="w-full h-64 object-cover hover:scale-110 transition-transform duration-300 ease-in-out" />
          </div>
          {{/each}}
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if testimonials.value.testimonials.items}}
    {{#unless testimonials.meta.hide}}
    <section id="testimonials" class="py-24 sm:py-32 bg-[var(--surface)]/10">
      <div class="max-w-6xl mx-auto px-4">
        <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)] text-center mb-16" data-aos="fade-up">
          {{default testimonials.value.testimonials.section_title 'What Our Patients Say'}}
        </h2>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
          {{#each testimonials.value.testimonials.items}}
          <div class="bg-[var(--background)] border border-[var(--border)] rounded-lg p-8" data-aos="fade-up" data-aos-delay="100">
            <p class="text-[var(--text-secondary)] leading-relaxed italic">
              "{{this.recommendationText}}"
            </p>
            <p class="font-semibold text-[var(--accent)] mt-6">- {{this.recommenderName}}</p>
          </div>
          {{/each}}
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if faq.value.faq.items}}
    {{#unless faq.meta.hide}}
    <section id="faq" class="py-24 sm:py-32">
      <div class="max-w-3xl mx-auto px-4">
        <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)] text-center mb-16" data-aos="fade-up">
          {{default faq.value.faq.section_title 'Frequently Asked Questions'}}
        </h2>
        <div class="space-y-6" data-aos="fade-up" data-aos-delay="100">
          {{#each faq.value.faq.items}}
          <div class="border-b border-[var(--border)]">
            <button data-accordion-button class="w-full flex justify-between items-center text-left py-5">
              <span class="text-lg font-semibold text-[var(--text-secondary)]">{{this.question}}</span>
              <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2"
                stroke="currentColor" class="w-6 h-6 text-[var(--accent)] transition-transform duration-300 transform">
                <path stroke-linecap="round" stroke-linejoin="round" d="M12 4.5v15m7.5-7.5h-15" />
              </svg>
            </button>
            <div data-accordion-answer class="max-h-0 overflow-hidden transition-max-height duration-500 ease-in-out">
              <p class="pb-5 pr-10 text-[var(--text-secondary)] leading-relaxed">
                {{this.answer}}
              </p>
            </div>
          </div>
          {{/each}}
        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}

    {{#if contacts.value.contacts.contactForm}}
    {{#unless contacts.meta.hide}}
    <section id="contact" class="py-24 sm:py-32 bg-[var(--surface)]">
      <div class="max-w-6xl mx-auto px-4">
        <div class="grid grid-cols-1 md:grid-cols-2 gap-12 items-center" data-aos="fade-up">

          <div data-aos="zoom-in">
            <img
              src="{{default contacts.value.contacts.image 'https://px-s3-img.pixpa.com/com/large/627114/627114-1651243627-florian-schmidinger-b-79noqf95i-unsplash.jpg?auto=format&fit=crop&w=2370&q=80'}}"
              alt="Clinic Technology" class="w-full h-full object-cover rounded-lg shadow-lg">
          </div>

          <form class="space-y-6" data-aos="fade-up">
            <h2 class="text-4xl md:text-5xl font-heading tracking-wide text-[var(--accent)] mb-8">
             {{default contacts.value.contacts.contactFormHeading 'Request an Appointment'}} 
            </h2>

            <div class="grid grid-cols-1 sm:grid-cols-2 gap-6">
              <div>
                <label for="first-name" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">First Name *</label>
                <input type="text" id="first-name" name="firstName" required
                  class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors">
              </div>
              <div>
                <label for="last-name" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">Last Name *</label>
                <input type="text" id="last-name" name="lastName" required
                  class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors">
              </div>
            </div>

            <div class="grid grid-cols-1 sm:grid-cols-2 gap-6">
              <div>
                <label for="email" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">Email *</label>
                <input type="email" id="email" name="email" required
                  class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors">
              </div>
              <div>
                <label for="phone" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">Phone</label>
                <input type="tel" id="phone" name="phone"
                  class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors">
              </div>
            </div>

            <div>
              <label for="date" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">Preferred Date</label>
              <input type="date" id="date" name="date"
                class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors"
                style="color-scheme: dark;">
            </div>

            <div>
              <label for="message" class="block text-sm font-medium text-[var(--text-secondary)] mb-2">{{default contacts.value.contacts.messageInputLabel 'Message'}} *</label>
              <textarea id="message" name="message" rows="4" required
                placeholder="{{default contacts.value.contacts.messageInputPlaceholder 'Briefly describe the reason for your visit...'}}"
                class="block w-full bg-transparent border border-[var(--border)] rounded-lg p-3 focus:ring-[var(--accent)] focus:border-[var(--accent)] transition-colors placeholder:text-white/30"></textarea>
            </div>

            <div>
              <button type="submit"
                class="w-full font-accent text-base font-bold text-[var(--background)] bg-[var(--accent)] rounded-lg px-8 py-4 hover:bg-[var(--accent)] transition-colors">
               {{default contacts.value.contacts.contactFormButtonText 'Request Appointment'}} 
              </button>
            </div>
          </form>

        </div>
      </div>
    </section>
    {{/unless}}
    {{/if}}
{{#if contacts.value.contacts}}
{{#unless contacts.meta.hide}}
<section class="py-24 sm:py-32">
  <div class="max-w-6xl mx-auto px-4">

    <div class="flex flex-col md:flex-row flex-wrap justify-center gap-12 text-center md:text-left">

      {{#if contacts.value.contacts.addressText}}
      <div class="w-full md:max-w-sm" data-aos="fade-up" data-aos-delay="100">
        <div class="flex justify-center md:justify-start">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
            stroke="currentColor" class="w-10 h-10 text-[var(--accent)]">
            <path stroke-linecap="round" stroke-linejoin="round" d="M15 10.5a3 3 0 1 1-6 0 3 3 0 0 1 6 0Z" />
            <path stroke-linecap="round" stroke-linejoin="round"
              d="M19.5 10.5c0 7.142-7.5 11.25-7.5 11.25S4.5 17.642 4.5 10.5a7.5 7.5 0 1 1 15 0Z" />
          </svg>
        </div>
        <h3 class="text-2xl font-heading text-[var(--accent)] mt-6 mb-4">Address</h3>

        <p class="text-[var(--text-secondary)] leading-relaxed break-words">
          {{contacts.value.contacts.addressText}}
        </p>
      </div>
      {{/if}}

      <div class="w-full md:max-w-sm" data-aos="fade-up" data-aos-delay="200">
        <div class="flex justify-center md:justify-start">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
            stroke="currentColor" class="w-10 h-10 text-[var(--accent)]">
            <path stroke-linecap="round" stroke-linejoin="round"
              d="M2.25 6.75c0 8.284 6.716 15 15 15h2.25a2.25 2.25 0 0 0 2.25-2.25v-1.372c0-.516-.351-.966-.852-1.091l-4.423-1.106c-.44-.11-.902.055-1.173.417l-.97 1.293c-2.892-1.46-5.17-3.743-6.63-6.63l1.293-.97c.363-.271.527-.734.417-1.173L6.963 3.102a1.125 1.125 0 0 0-1.091-.852H4.5A2.25 2.25 0 0 0 2.25 6.75Z" />
          </svg>
        </div>
        <h3 class="text-2xl font-heading text-[var(--accent)] mt-6 mb-4">Contact Us</h3>

        <div class="text-[var(--text-secondary)] leading-relaxed space-y-1 break-words">
          {{#if contacts.value.contacts.phone}}
          <p>Phone: {{contacts.value.contacts.phone}}</p>
          {{/if}}
          {{#if contacts.value.contacts.email}}
          <p>Email: {{contacts.value.contacts.email}}</p>
          {{/if}}
        </div>
      </div>

      {{#if contacts.value.contacts.openingHours}}
      <div class="w-full md:max-w-sm" data-aos="fade-up" data-aos-delay="300">
        <div class="flex justify-center md:justify-start">
          <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
            stroke="currentColor" class="w-10 h-10 text-[var(--accent)]">
            <path stroke-linecap="round" stroke-linejoin="round" d="M12 6v6h4.5m4.5 0a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z" />
          </svg>
        </div>
        <h3 class="text-2xl font-heading text-[var(--accent)] mt-6 mb-4">Opening Hours</h3>
        <ul class="text-[var(--text-secondary)] leading-relaxed space-y-1">
          {{#each contacts.value.contacts.openingHours}}
          <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      {{/if}}

    </div>
  </div>
</section>
{{/unless}}
{{/if}}
    
    {{#if map.value.map.url}}
    {{#unless map.meta.hide}}
    <section class="h-[450px] md:h-[550px]">
      <iframe
        src="{{default map.value.map.url 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3000.77661604858!2d-71.954200684579!3d41.008433979300!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89e8ac014f526b77%3A0x438f46e50868f6b!2sLake%20Montauk!5e0!3m2!1sen!2sus!4v1678886400000!5m2!1sen!2sus'}}"
        width="100%" height="100%" style="border:0;" allowfullscreen="" loading="lazy"
        referrerpolicy="no-referrer-when-downgrade">
      </iframe>
    </section>
    {{/unless}}
    {{/if}}

  </main>

  <footer class="py-12 border-t border-[var(--border)]">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8 text-center sm:flex sm:justify-between sm:items-center">
      <p class="text-sm text-[var(--text-secondary)]">
        Â© {{currentYear}} {{personal_info.value.firstName}} {{personal_info.value.lastName}}, All rights reserved.
      </p>

      {{#if social.value.social.items}}
      {{#unless social.meta.hide}}
      <div class="flex justify-center space-x-6 mt-4 sm:mt-0">
        {{#each social.value.social.items}}
        <a href="{{this.url}}" class="text-[var(--text-secondary)] hover:text-[var(--accent)] transition-colors" aria-label="{{this.label}}">
        {{{socialIcon url=this.url
        label=this.label size=40 className="size-12"}}}
        </a>
       {{/each}}
      </div>
      {{/unless}}
      {{/if}}
    </div>
  </footer>


  <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
  <script>
    // Initialize AOS
    AOS.init({
      duration: 700,
      once: true,
      offset: 100
    });

    // Mobile Menu & Accordion
    document.addEventListener('DOMContentLoaded', () => {
      // Mobile Menu
      const menuButton = document.getElementById('menu-button');
      const mobileMenu = document.getElementById('mobile-menu');
      const mobileNavLinks = mobileMenu.querySelectorAll('a');

      menuButton.addEventListener('click', () => {
        const isExpanded = menuButton.getAttribute('aria-expanded') === 'true';
        menuButton.setAttribute('aria-expanded', !isExpanded);
        mobileMenu.classList.toggle('hidden');
      });

      mobileNavLinks.forEach(link => {
        link.addEventListener('click', () => {
          menuButton.setAttribute('aria-expanded', 'false');
          mobileMenu.classList.add('hidden');
        });
      });

      // Accordion
      const accordionButtons = document.querySelectorAll('[data-accordion-button]');

      accordionButtons.forEach(button => {
        button.addEventListener('click', () => {
          const answer = button.nextElementSibling;
          const icon = button.querySelector('svg');

          if (answer.style.maxHeight) {
            // Close
            answer.style.maxHeight = null;
            icon.classList.remove('rotate-45');
          } else {
            // Open
            answer.style.maxHeight = answer.scrollHeight + 'px';
            icon.classList.add('rotate-45');
          }
        });
      });
    });
  </script>

</body>

</html>